# Gemfile for Jekyll site
source "https://rubygems.org"

gem "jekyll", "~> 4.3"
gem "jekyll-sitemap"
gem "jekyll-seo-tag"
gem "jekyll-feed"

# Performance and optimization
gem "jekyll-compress-images"
gem "jekyll-minifier"

group :jekyll_plugins do
  gem "jekyll-sitemap"
  gem "jekyll-seo-tag"
  gem "jekyll-feed"
end